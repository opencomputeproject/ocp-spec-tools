$endfor$
</head>

<body class="markdown-body">
<div align=center>
<img src="extra/ocp-pandoc-resources/img/ocp_logo.svg" width="500px" alt="Logo">

$if(title)$
<header id="title-block-header">
<h1 class="title">$title$</h1> 

$if(subtitle)$
<p class="subtitle">$subtitle$</p>
$endif$

$for(author)$
<p class="author">$author$</p>
$endfor$
$if(version-html)$
<strong>
	<p>Version: $version-html$</p>
</strong>
$endif$
</header>
</div>
<div id="bottom">
<strong>
$if(date)$
	<p>Date: $date$</p>
$endif$
$if(doc-status)$
	<p>Document Status: $doc-status$</p>
$endif$
$if(doc-lang)$
	<p>Document Language: $doc-lang$</span></p>
$endif$
</strong>
</div>
$endif$
$if(copyright-year)$
  <div class="copyright">
    Copyright Notice</br>
    Copyright &copy; $copyright-year$ OCP. All rights reserved.
  </div>
$endif$
$if(toc)$
<h1>Table of contents</h1>
<nav class="toc">
$table-of-contents$
</nav>
$endif$
<article>
$body$
</article>
$for(include-after)$
$include-after$
$endfor$
</body>
</html>